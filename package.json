{
  "name": "ro-crate-as-data",
  "type": "module",
  "version": "1.0.0",
  "description": "Tools for extracting data from RO-Crates, including HTML pages and CSV",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all -p dev:*",
    "build": "run-s build:*",
    "dev:11ty": "eleventy --serve",
    "dev:css": "tailwindcss -i src/assets/css/tailwind.css -o dist/style.css  --watch --postcss",
    "build:11ty": "eleventy",
    "build:css": "tailwindcss -i src/assets/css/tailwind.css -o dist/.css --postcss"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "eleventy": "^1.0.6",
    "fs-extra": "^11.2.0",
    "level": "^8.0.1",
    "ro-crate": "^3.3.6"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3"
  }
}
